{
  "manifest_version": 2,

  "name": "Github Repository Popup Extension",
  "description": "This extension makes it possible to popup github repository information.",
  "version": "1.0",
  "content_scripts" : [
    {
      "matches" :["https://github.com/*"],
      "css" : [],
      "js" : ["src/lib/jquery-2.0.3.min.js", "src/js/main.js"]
    }
  ],
  "web_accessible_resources" : [
    "resources/colors.json"
  ],
  "permissions": [
    "https://github.com/",
    "https://api.github.com/"
  ]
}
